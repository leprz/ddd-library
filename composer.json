{
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "php": ">=7.2.5",
        "symfony/console": "5.2.*",
        "symfony/dependency-injection": "^5.0",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-kernel": "^5.0",
        "symfony/yaml": "5.2.*"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": [
                "packages/circulation/tests/",
                "packages/finances/tests/",
                "packages/stocks/tests/"
            ]
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symplify/monorepo-builder": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "packages/circulation/src/",
                "packages/finances/src/",
                "packages/stocks/src/"
            ]
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "your-monorepo/first-package": "self.version",
        "your-monorepo/second-package": "self.version"
    },
    "name": "leprz/ddd-library",
    "description": "Example ddd project",
    "minimum-stability": "dev",
    "prefer-stable": true
}
